{
    "manifest_version": 2,
    "name": "Discord PGP",
    "version": "0.0.1",
    "description": "PGP messaging for Discord",
    "icons": {
        "48": "icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://discord.com/channels/@me/*"],
            "js": ["discord.js"]
        }
    ],
    "permissions": [
        "storage",
        "webRequest"
    ]
}